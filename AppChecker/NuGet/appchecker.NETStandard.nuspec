<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>AppChecker.NETStandard</id>
    <version>3.0.0</version>
    <authors>BlueJeans Development Team</authors>
    <owners>BlueJeans Development Team</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>AppChecker is a software test library.</description>
    <copyright>Public Domain</copyright>
    <tags></tags>
    <dependencies>
      <group targetFramework=".NETStandard1.0" />
    </dependencies>
  </metadata>

  <files>
    <!-- build -->
    <file src="AppChecker.NETStandard.targets" target="build\netstandard1.0"/>

    <!-- lib -->
    <file src="..\AppChecker\bin\release\AppChecker.dll" target="lib\netstandard1.0"/>

    <!-- runtimes -->
    <file src="..\AppChecker\bin\x86\release\AppChecker.dll" target="runtimes\win-x86\lib"/>
    <file src="..\AppChecker\bin\x64\release\AppChecker.dll" target="runtimes\win-x64\lib"/>
  </files>
  
</package>